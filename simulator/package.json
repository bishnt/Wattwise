{
  "name": "power-consumption-simulator",
  "version": "1.0.0",
  "description": "Realistic household power consumption simulator for research",
  "main": "meter_simulator.js",
  "scripts": {
    "start": "node meter_simulator.js",
    "simulate:normal": "node meter_simulator.js --profile normal",
    "simulate:high": "node meter_simulator.js --profile high",
    "simulate:low": "node meter_simulator.js --profile low",
    "simulate:vacation": "node meter_simulator.js --profile vacation"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0"
  },
  "keywords": ["energy", "simulator", "smart-meter", "power-consumption"],
  "author": "Research Team",
  "license": "MIT"
}